<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.event.ticketing.system.eventticketingsystem.controller.MainMenuController"
      alignment="CENTER"
      spacing="20"
      stylesheets="@login-style.css">

<Button fx:id="addTicketButton" text="Add Ticket"
        onAction="#handleAddTicketButton"
        styleClass="main-menu-button" />

<Button fx:id="buyTicketButton" text="Buy Ticket"
        onAction="#handleBuyTicketButton"
        styleClass="main-menu-button" />
</VBox>


